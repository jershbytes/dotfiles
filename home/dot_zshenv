export ZSH="$HOME/.oh-my-zsh" # ZSH installation path
export LANG="en_US.UTF-8" # Language

### PATHS ###
export PATH="$PATH:$HOME/.local/bin" # MkDocs
export PATH="/usr/bin/ansible-lint"$PATH # Ansible Lint

### APP CONFIGS
export HOMEBREW_NO_ENV_HINTS=true # Disable Homebrew environment hints
export POSH_THEME="$HOME/.config/ohmyposh/bubba.omp.json" # OhMyPosh config
export EDITOR="code --wait" # VSCode
