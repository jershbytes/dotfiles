{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "blocks": [
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "properties": {
            "cache_duration": "none"
          },
          "leading_diamond": "\ue0b6",
          "trailing_diamond": "\ue0b4",
          "template": " ⚡",
          "foreground": "#F1FA8C",
          "background": "#BD93F9",
          "type": "root",
          "style": "diamond"
        },
        {
          "properties": {
            "cache_duration": "none"
          },
          "leading_diamond": "<#6272A4>\ue0b6</><transparent,#6272A4>\ue0b6</>",
          "trailing_diamond": "\ue0b4",
          "template": "{{ if .WSL }}WSL {{ end }}{{.Icon}}",
          "foreground": "#F8F8F2",
          "background": "transparent",
          "type": "os",
          "style": "diamond"
        },
        {
          "properties": {
            "cache_duration": "none",
            "folder_icon": "\uf07b",
            "folder_separator_icon": "/",
            "home_icon": "~",
            "style": "agnoster"
          },
          "leading_diamond": "<transparent>\ue0b4</>",
          "trailing_diamond": "\ue0b4",
          "template": " {{ path .Path .Location }} ",
          "foreground": "#F8F8F2",
          "background": "#6272A4",
          "type": "path",
          "style": "diamond"
        },
        {
          "foreground": "#282A36",
          "background": "#BD93F9",
          "properties": {
            "branch_icon": " \ue725 ",
            "cherry_pick_icon": "✓ ",
            "commit_icon": "\u25b7 ",
            "fetch_stash_count": true,
            "fetch_status": true,
            "fetch_upstream_icon": true,
            "merge_icon": "\u25f4 ",
            "no_commits_icon": "[no commits]",
            "rebase_icon": "\u2c62 ",
            "tag_icon": "\u25b6 "
          },
          "style": "diamond",
          "leading_diamond": "<transparent>\ue0b4</>",
          "trailing_diamond": "\ue0b4",
          "template": " {{ url .UpstreamIcon .UpstreamURL }}<i>{{ url .RepoName .UpstreamURL }}</i><b>{{ .HEAD }}</b>{{ if .BranchStatus }} {{ .BranchStatus }}{{ end }}{{ if or (.Working.Changed) (.Staging.Changed) }} |{{ end }}{{ if .Staging.Changed }}<#FFB86C> \uf046 {{ .Staging.String }}</>{{ end }}{{ if and (.Working.Changed) (.Staging.Changed) }} |{{ end }}{{ if .Working.Changed }}<#FF5555> \uf044 {{ .Working.String }}</>{{ end }}{{ if gt .StashCount 0 }}<#F1FA8C> \ueb4b {{ .StashCount }}</>{{ end }} ",
          "background_templates": [
            "{{ if or (.Working.Changed) (.Staging.Changed) }}#FFB86C{{ end }}",
            "{{ if and (gt .Ahead 0) (gt .Behind 0) }}#FF79C6{{ end }}",
            "{{ if gt .Ahead 0 }}#8BE9FD{{ end }}",
            "{{ if gt .Behind 0 }}#50FA7B{{ end }}"
          ],
          "type": "git"
        },
        {
          "properties": {
            "cache_duration": "none",
            "style": "dallas",
            "threshold": 0
          },
          "leading_diamond": "<transparent>\ue0b4</>",
          "trailing_diamond": "\ue0b4",
          "template": " {{ .FormattedMs }}s ",
          "foreground": "#F8F8F2",
          "background": "#44475A",
          "type": "executiontime",
          "style": "diamond"
        },
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": "<transparent> \uf12a</> {{ reason .Code }} ",
          "foreground": "#F8F8F2",
          "powerline_symbol": "\ue0b4",
          "background": "#FF5555",
          "type": "status",
          "style": "powerline"
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "right",
      "segments": [
        {
          "properties": {
            "cache_duration": "none",
            "mapped_shell_names": {
              "bash": "\udb84\udd83 bash ",
              "pwsh": "\ue683 pwsh "
            }
          },
          "leading_diamond": "\ue0b6",
          "trailing_diamond": "<transparent>\ue0b6</>",
          "template": " <b>{{ .Name }}</b>",
          "foreground": "#8BE9FD",
          "background": "#282A36",
          "type": "shell",
          "style": "diamond"
        },
        {
          "properties": {
            "cache_duration": "none",
            "display_default": true,
            "folder_name_fallback": false,
            "home_enabled": true
          },
          "leading_diamond": "\ue0b6",
          "trailing_diamond": "<transparent>\ue0b6</>",
          "template": " \ue73c {{ if .Error }}{{ .Error }}{{ else }}{{ if .Venv }}{{ .Venv }} {{ end }}{{ .Full }}{{ end }} ",
          "foreground": "#FF79C6",
          "background": "#282A36",
          "type": "python",
          "style": "diamond"
        },
        {
          "properties": {
            "cache_duration": "none",
            "time_format": "15:04:05"
          },
          "leading_diamond": "\ue0b6",
          "template": " {{ .CurrentDate | date .Format }} <#282A36,transparent>\ue0b4</>",
          "foreground": "#50FA7B",
          "background": "#282A36",
          "type": "time",
          "style": "diamond"
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": " \ue65e {{ .Full }} ",
          "foreground": "#8BE9FD",
          "background": "#282A36",
          "type": "go",
          "style": "powerline"
        },
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": " \ue77f {{ .Full }} ",
          "foreground": "#8BE9FD",
          "background": "#282A36",
          "type": "dotnet",
          "style": "powerline"
        },
        {
          "properties": {
            "always_enabled": true,
            "cache_duration": "none"
          },
          "template": "❯",
          "foreground": "#F8F8F2",
          "type": "status",
          "style": "plain",
          "foreground_templates": [
            "{{ if gt .Code 0 }}#FF5555{{ end }}"
          ]
        }
      ],
      "newline": true
    }
  ],
  "version": 3,
  "shell_integration": true,
  "patch_pwsh_bleed": true,
  "final_space": true
}
