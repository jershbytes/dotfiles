{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "blocks": [
    {
      "alignment": "left",
      "segments": [
        {
          "foreground": "#50FA7B",
          "style": "plain",
          "template": "\ueb99 {{ .UserName }} on",
          "type": "session"
        },
        {
          "foreground": "#8BE9FD",
          "properties": {
            "folder_separator_icon": "/",
            "style": "full"
          },
          "style": "plain",
          "template": " \uf07b {{ .Path }} ",
          "type": "path"
        },
        {
          "foreground": "#BD93F9",
          "properties": {
            "fetch_stash_count": true,
            "fetch_upstream_icon": true,
            "display_status": true
          },
          "style": "plain",
          "template": "{{ if .RepoUrl }}[{{ if eq .RepoHost \"github.com\" }}\uf09b{{ else if eq .RepoHost \"gitlab.com\" }}\uf296{{ else if eq .RepoHost \"bitbucket.org\" }}\uf171{{ else if eq .RepoHost \"dev.azure.com\" }}\uf375{{ else }}\ue725{{ end }}]({{ .RepoUrl }}) {{ end }}{{ .HEAD }}{{ if gt .StashCount 0 }} \ueb4b {{ .StashCount }}{{ end }} ",
          "type": "git"
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "foreground": "#FF5555",
          "style": "plain",
          "template": "\ue3bf ",
          "type": "root"
        },
        {
          "foreground": "#FF79C6",
          "style": "plain",
          "template": "# ",
          "type": "text"
        }
      ],
      "type": "prompt"
    }
  ],
  "version": 3
}
